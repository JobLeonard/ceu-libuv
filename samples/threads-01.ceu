par/or do
    async do
        loop i do
            _sleep(1);
            _printf("[%d] async\n", i);
        end
    end
with
    async/thread do
        loop i in 5 do
            _sleep(1);
            _printf("[%d] thread\n", i);
        end
    end
with
    loop i do
        await 1s;
        _printf("[%d] sync\n", i);
    end
end

escape 0;
